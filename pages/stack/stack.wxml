<view>
  <view class="line"></view>
  <scroll-view wx:for="{{stack}}" wx:key="{{index}}" scroll-y="{{true}}">
    <navigator class="stack-view" url="booklist/booklist?id={{index}}">
      <image src="{{item.typeCover}}" class="img" />
      <text class="name">{{item.bookTypes}}</text>
      <text class="number">{{item.bookNumb}}</text>
      <text class="arrow">></text>
    </navigator>
    <view class="line"></view>
  </scroll-view>
</view>